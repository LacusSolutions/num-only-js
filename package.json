  "repository": {
    "type": "git",
    "url": "git+https://github.com/juliolmuller/num-only-js.git"
  },
  "bugs": {
    "url": "https://github.com/juliolmuller/num-only-js/issues"
  },
  "homepage": "https://github.com/juliolmuller/num-only-js#readme",
  "main": "build/index.cjs.js",
