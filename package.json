{
  "name": "@lacussoft/num-only",
  "version": "1.0.6",
  "description": "Function to stringify and remove non-numeric characters of items.",
  "author": "Julio L. Muller",
  "license": "MIT",
  "keywords": [
    "lacussoft",
    "strip",
    "remove",
    "extract",
    "numbers",
    "only",
    "strings"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/juliolmuller/num-only.git"
  },
  "bugs": {
    "url": "https://github.com/juliolmuller/num-only/issues"
  },
  "homepage": "https://github.com/juliolmuller/num-only#readme",
  "typings": "build/types.d.ts",
  "main": "build/index.js",
  "scripts": {
    "build:browser": "rm -rf dist/ && webpack --mode=production",
    "build:node": "rm -rf build/ && babel src/ --out-dir build/ && cp src/*.d.ts build/",
    "build": "npm run build:browser && npm run build:node",
    "lint": "eslint --fix src/**/*.js",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@lacussoft/eslint-config": "^1.0.0",
    "babel-loader": "^8.2.2",
    "eslint": "^7.15.0",
    "jest": "^26.6.3",
    "webpack": "^5.10.3",
    "webpack-cli": "^4.2.0"
  }
}
